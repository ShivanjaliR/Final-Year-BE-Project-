<?xml version="1.0" encoding="utf-8"?>
	
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
   	android:layout_height="fill_parent" 
   	android:background="@drawable/back78">
    	
	<RelativeLayout
	android:layout_width="fill_parent" android:layout_height="1104dp">
		
	<TextView
		android:id="@+id/text_productid" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_alignParentTop="true" 
		android:layout_alignParentLeft="true"		 
		android:layout_marginTop="10dp"
		android:layout_marginLeft="10dip"
		android:text="Product Id"
		android:textStyle="bold"></TextView>
		
	<EditText 
		android:id="@+id/edit_productid"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:inputType="text" 
        android:textSize="16sp" 
        android:layout_alignParentLeft="true" 
		android:layout_alignParentRight="true"		
		android:layout_below="@+id/text_productid"
		android:layout_marginTop="10dp"></EditText>
	
	<TextView 
		android:id="@+id/text_name" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:layout_below="@+id/edit_productid"  
		android:layout_marginTop="10dp"
        android:layout_marginLeft="10dip"        
        android:text="Name"
        android:textStyle="bold">
		</TextView>
		
	<EditText 
		android:id="@+id/edit_name"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:inputType="text" 
        android:textSize="16sp"   
		android:layout_alignParentLeft="true" 
		android:layout_alignParentRight="true"		
		android:layout_below="@+id/text_name"
		android:layout_marginTop="10dp"></EditText>
	
	<TextView 
		android:id="@+id/text_quantity"
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"		
		android:textStyle="bold" 
		android:text="Quantity"
		android:layout_below="@+id/edit_name"
		android:layout_marginTop="10dp"
		android:layout_marginLeft="10dip"></TextView>
	
	<EditText 
		android:id="@+id/edit_quantity"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:inputType="number" 
        android:textSize="16sp"  
		android:layout_alignParentRight="true" 
		android:layout_alignParentLeft="true"		
		android:layout_marginTop="10dp"
		android:layout_below="@+id/text_quantity"></EditText>
	
	<TextView 
		android:id="@+id/text_price" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_quantity" 
		android:layout_alignParentLeft="true"		
		android:layout_marginTop="10dp"
		android:layout_marginLeft="10dp"
		android:textStyle="bold"
		android:text="Price"></TextView>
	
	<EditText
		android:id="@+id/edit_price" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:inputType="number"
		android:textSize="16sp" 
		android:layout_below="@+id/text_price" 
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true"
		lignParentBottom="true"
		android:layout_marginTop="10dp"
		></EditText>
		
	<TextView 
		android:id="@+id/text_category"
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_price" 
		android:layout_alignParentLeft="true"		
		android:layout_marginTop="10dp"
		android:layout_marginLeft="10dp"
		android:textStyle="bold" 
		android:text="Category"></TextView>
	<EditText
		android:id="@+id/edit_category" 
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:inputType="text"
		android:textSize="16sp"  
		android:layout_below="@+id/text_category" 
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true"
		android:layout_marginTop="10dp"
		></EditText>
		
	<TextView 
		android:id="@+id/text_notes" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_category" 
		android:layout_alignParentLeft="true" 
		android:layout_marginTop="10dp"
		android:layout_marginLeft="10dp"
		android:textStyle="bold"		
		android:text="Product Description"></TextView>

	<EditText 
		android:layout_below="@+id/text_notes"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:inputType="textMultiLine"
		android:textSize="16sp"		 
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true" 
		android:id="@+id/edit_notes"
		android:layout_marginTop="10dp">
		<requestFocus></requestFocus>
	</EditText>
	
	<TextView 
		android:id="@+id/text_manufact" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_notes" 
		android:layout_alignParentLeft="true"
		android:textStyle="bold"  
		android:layout_marginTop="10dp"
		android:layout_marginLeft="10dp"		
		android:text="Manufacturer"></TextView>
	
	<EditText 
		android:id="@+id/edit_manufact"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:inputType="text"
		android:textSize="16sp" 
		android:layout_below="@+id/text_manufact"		
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true"
		android:layout_marginTop="10dp"></EditText>
	
	<TextView 
		android:id="@+id/text_shippingcost"
		android:textStyle="bold" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:text="Shipping Cost"		 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_manufact" 
		android:layout_alignLeft="@+id/text_manufact"
		android:layout_marginTop="10dp"></TextView>

	<EditText
		android:id="@+id/edit_shippingcost" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:inputType="text"
		android:textSize="16sp"  
		android:layout_below="@+id/text_shippingcost" 
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true"
		android:layout_marginTop="10dp" 
		></EditText>
	
	
	<TextView 
		android:id="@+id/text_shippingtime"
		android:textStyle="bold" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:text="Shipping Time"		 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_shippingcost" 
		android:layout_alignLeft="@+id/text_shippingcost"
		android:layout_marginTop="10dp"></TextView>

	<EditText
		android:id="@+id/edit_shippingtime" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:inputType="text"
		android:textSize="16sp"  
		android:layout_below="@+id/text_shippingtime" 
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true"
		android:layout_marginTop="10dp" 
		></EditText>
		
	
	<TextView
		android:id="@+id/text_vendoraccno" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_shippingtime" 
		android:layout_alignLeft="@+id/text_shippingtime"
		android:layout_marginTop="10dp" 		 
		android:text="Vendor's Account No."
		android:textStyle="bold"></TextView>
	
	<EditText
		android:id="@+id/edit_vendoraccno" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:inputType="text"
		android:textSize="16sp"  
		android:layout_below="@+id/text_vendoraccno" 
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true"
		android:layout_marginTop="10dp" 
		android:enabled="false"></EditText>
	
	<TextView 
		android:id="@+id/text_discount" 
		android:textAppearance="?android:attr/textAppearanceMedium" 
		android:text="Discount"		
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_vendoraccno" 
		android:layout_alignParentLeft="true"
		android:layout_marginTop="10dp"
		android:layout_marginLeft="10dp"
		android:textStyle="bold"></TextView>
	
	<EditText
		android:id="@+id/edit_discount" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/text_discount" 
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true"
		android:layout_marginTop="10dp" 
		></EditText>
	
	<Button 
		android:id="@+id/button_addproduct"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content" 
		android:layout_below="@+id/edit_discount" 
		android:layout_alignLeft="@+id/text_discount" 
		android:layout_marginTop="40dp"		 
		android:text="Add Product" 
		android:layout_alignRight="@+id/text_shippingtime"
		android:onClick="check">
		</Button>
	<Button android:id="@+id/button_cancel"
		 android:text="Cancel" 
		 android:layout_height="wrap_content" 
		 android:layout_width="wrap_content" 
		 android:layout_alignBaseline="@+id/button_addproduct" 
		 android:layout_alignBottom="@+id/button_addproduct" 
		 android:layout_alignParentRight="true" 
		 android:layout_marginRight="26dp" 
		 android:layout_toRightOf="@+id/text_notes"
		  android:onClick="cancel">
		  </Button>

</RelativeLayout>
</ScrollView>